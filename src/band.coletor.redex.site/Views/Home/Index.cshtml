                    </button>
                </div>
            </div>
            <div class="card-body">
                <div class="row">
                    <div class="col-md-12">
                        <a href="@Url.Action("Cadastrar", "PreRegistro")" class="btn btn-light btn-grey btn-lg btn-block text-left mb-3 font-home pre-line" style="text-align:left">
                            <img src="~/Content/img/caminhao-entrada.png" />Pré-Registro
                        </a>
                        <a href="@Url.Action("Index", "SaidaCaminhao")" class="btn btn-light btn-grey btn-lg btn-block text-left mb-3 font-home pre-line">
                            <img src="~/Content/img/caminhao-saida32.png" />Registrar Saída do Caminhão
                        </a>
                        <a href="@Url.Content("~/TalieColetorDescarga/index/dd")" class="btn btn-light btn-grey btn-lg btn-block text-left mb-3 font-home pre-line">
                            <img src="~/Content/img/descarga.png" />Descarga Armazém
                        </a>
                        <a href="@Url.Content("~/TalieColetorDescarga/index/cd")" class="btn btn-light btn-grey btn-lg btn-block text-left mb-3 font-home pre-line">
                            <img src="~/Content/img/descarga.png" />Descarga CD
                        </a>
                        <a href="@Url.Content("~/Estufagem")" class="btn btn-light btn-grey btn-lg btn-block text-left mb-3 font-home pre-line">
                            <img src="~/Content/img/estufagem.png" />Estufagem
                        </a>

                        <!-- Telas adicionais -->
                        <a href="@Url.Action("Index", "CarregamentoCargaSolta")" class="btn btn-light btn-grey btn-lg btn-block text-left mb-3 font-home pre-line">
                            <img src="~/Content/img/descarga.png" />Carregamento Carga Solta
                        </a>

                        <a href="#" class="btn btn-light btn-grey btn-lg btn-block text-left mb-3 font-home pre-line disabled">
                            <img src="~/Content/img/caminhao-entrada.png" />Descarga Exportação
                        </a>
                        <a href="#" class="btn btn-light btn-grey btn-lg btn-block text-left mb-3 font-home pre-line disabled">
                            <img src="~/Content/img/caminhao-entrada.png" />Estufagem Container
                        </a>
                        <a href="#" class="btn btn-light btn-grey btn-lg btn-block text-left mb-3 font-home pre-line disabled">
