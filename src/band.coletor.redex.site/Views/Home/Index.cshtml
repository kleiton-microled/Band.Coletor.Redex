@using Band.Coletor.Redex.Site.Extensions;
@using System.Configuration;

@{
    ViewBag.Title = "Band :: Coletor";
    var coletorPath = ConfigurationManager.AppSettings["ColetorConsultasPath"];
    var pathMovimentacaoConteiner = coletorPath + "?EXTERNO=InventarioCNTR.aspx&CONTENT=" + User.ObterIdEncriptado();
    var pathMovimentacaoCargaSolta = coletorPath + "?EXTERNO=InventarioCS.aspx&CONTENT=" + User.ObterIdEncriptado();
    var pathEstufagem = coletorPath + "?EXTERNO=Estufagem.aspx&CONTENT=" + User.ObterIdEncriptado();
    var pathDescarga = coletorPath + "?EXTERNO=Talie_Descarga_Registro.aspx&CONTENT=" + User.ObterIdEncriptado();
    var pathDescargaArmazem = coletorPath + "?EXTERNO=TalieColetorDescarga.aspx?dd=1;cd=0&CONTENT=" + User.ObterIdEncriptado();
    var pathDescargaCD = coletorPath + "?EXTERNO=TalieColetorDescarga.aspx?dd=0;cd=1&CONTENT=" + User.ObterIdEncriptado();

}
<div class="row">
    <div class="col-12">

        <div class="card ml-0 mr-0 pl-0 pr-0">
            <div class="card-header">
                <i class="fa fa-list fa-lg"></i>
                Menu
                <div class="float-right top">
                    <button type="button" class="btn btn-box-tool" data-widget="collapse" data-toggle="tooltip" title="Collapse">
